<div class="new-note-form">
  <form method="POST" action="/create-note">
    <label class="label">Note Title</label>
    <input type="hidden" class="id-note-title" value={{data.userId}} name="id"></input>

    <div class="title">
      <input type="text" class="n-note-title" name="title"></input>
    </div>
    <label class="note-text">Write Your Note</label>
    <div class="text-note">
      <textarea class="note-text" name="note"></textarea>
    </div>
    <div class="submit-div">
      <input type="submit" name="submit">
    </div>
  </form>
</div>